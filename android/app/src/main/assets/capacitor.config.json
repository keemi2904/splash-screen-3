{
	"appId": "io.ionic.starter",
	"appName": "splash screen 3",
	"webDir": "www",
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#de0f17",
			"showSpinner": false,
			"androidSpinnerStyle": "small",
			"iosSpinnerStyle": "small",
			"splashFullScreen": true,
			"splashImmersive": true
		}
	}
}
